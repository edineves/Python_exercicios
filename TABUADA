#TABUADA
#Digite um numero e mostre na tela a tabuada deste numero.
n = int(input('Digite um numero'))
t = n*1
t1 = n*2
t2 = n*3
t3 = n*4
t4 = n*5
t5 = n*6
t6 = n*7
t7 = n*8
t8 = n*9
t9 = n*10

print(f' A tabuada de {n} Ã© : ')
print( f'{n} x 1 = {t}')
print( f'{n} x 2 = {t1}')
print( f'{n} x 3 = {t2}')
print( f'{n} x 4 = {t3}')
print( f'{n} x 5 = {t4}')
print( f'{n} x 6 = {t5}')
print( f'{n} x 7 = {t6}')
print( f'{n} x 8 = {t7}')
print( f'{n} x 9 = {t8}')
print( f'{n} x 10 = {t9}')

print( ' Fim ')

